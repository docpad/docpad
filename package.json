    "node": ">=0.10",
    "npm": ">=2.5.0"
  },
  "editions": [
    {
      "description": "Source + CoffeeScript + Require",
      "directory": "source",
      "entry": "docpad.coffee",
      "syntaxes": [
        "coffeescript",
        "require"
      ]
    },
    {
      "description": "CoffeeScript Compiled + ESNext + Require",
      "directory": "esnext",
      "entry": "docpad.js",
      "syntaxes": [
        "javascript",
        "esnext",
        "require"
      ]
    },
    {
      "description": "CoffeeScript Compiled + ES2015 + Require",
      "directory": "es2015",
      "entry": "docpad.js",
      "syntaxes": [
        "javascript",
        "es2015",
        "require"
      ]
    }
  ],
  "bin": {
    "docpad": "bin/docpad",
    "docpad-compile": "bin/docpad-compile",
    "docpad-debug": "bin/docpad-debug",
    "docpad-server": "bin/docpad-server",
    "docpad-trace": "bin/docpad-trace"
  },
